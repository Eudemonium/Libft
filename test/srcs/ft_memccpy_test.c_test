#include "libft.h"
#include "libft_test.h"

int	main(void)
{
	char	src[] = "test basic du memccpy !";
	char	*dst_1 = (char *)malloc(22);
	char	*dst_2 = (char *)malloc(22);
	char	*r1;
	char	*r2;
	r1 = (char *)memccpy(dst_1, src, 'm', 20);
	r2 = (char *)ft_memccpy(dst_2, src, 'm', 20);
	printf("==== ft_memccpy ====\n");
	printf("==== len = 20 ====\n");
	printf("%s\n",r1);
	printf("%s\n",r2);
	r1 = (char *)memccpy(dst_1, src, 'm', 0);
	r2 = (char *)ft_memccpy(dst_2, src, 'm', 0);
	printf("==== len = 0 ====\n");
	printf("%s\n",r1);
	printf("%s\n",r2);
	return (0);
}
